{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/auth/login",
      "DownstreamScheme": "http",
      "UseServiceDiscovery": true,
      "ServiceName": "authentication-svc",
      "SwaggerKey": "authentication-svc",
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": [ "Post" ],
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "CLS",
        "AllowedScopes": []
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "authentication-svc",
      "Config": [
        {
          "Name": "authentication-svc API",
          "Version": "v1",
          "Url": "http://192.169.0.108:9902/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": { "Type": "Eureka" },
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration"
  }
}  